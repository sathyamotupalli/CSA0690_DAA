#include<stdio.h>
int main()
{
	int n,m,i,k;
	printf("enter the first number= ");
	scanf("%d",&n);
	printf("enter the second number= ");
	scanf("%d",&m);
	if(n>=1 && m>=1){
	for(i=1;i<=n && i<=m ; i++)
	{
		if(n%i==0 && m%i==0)
		{
			k=i;
		}
	}
	printf("gcd=%d",k);
}
else
{
	printf("invalid inputs");
}
}
